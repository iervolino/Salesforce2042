    <validationRules>
        <fullName>DenialReasonRequired</fullName>
        <active>true</active>
        <description>Denial Reason Required when denying applicant.</description>
        <errorConditionFormula>ISPICKVAL( FundDenialReason__c , &quot;&quot;) &amp;&amp; ISPICKVAL (AwardStatus__c, &quot;Not Awarded&quot;) &amp;&amp;  $RecordType.Name =&apos;Scholarship&apos;</errorConditionFormula>
        <errorDisplayField>AwardStatus__c</errorDisplayField>
        <errorMessage>Please Select a Reason For Denial</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DenialWithNoAwardStatus</fullName>
        <active>true</active>
        <description>If a processor tries to deny an applicant without selection an award status the will get an error.</description>
        <errorConditionFormula>NOT (ISPICKVAL( FundDenialReason__c , &quot;&quot; ))  &amp;&amp;  ISPICKVAL( AwardStatus__c , &quot;Undecided&quot;) &amp;&amp;  $RecordType.Name =&apos;Scholarship&apos;</errorConditionFormula>
        <errorDisplayField>FundDenialReason__c</errorDisplayField>
        <errorMessage>Please make sure you choose an award status before choosing a fund denial reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FAFundDenialReason</fullName>
        <active>true</active>
        <description>FA Fund Denial Reason</description>
        <errorConditionFormula>AND(ISPICKVAL(IsApplicantQualified__c, &quot;Undecided&quot;), ISNULL(DateOfInterview__c),  ISNULL(CommitteeReviewDate__c ), ISPICKVAL( FundDenialReason__c , &quot;Not Qualified, Student Not Enrolling, Requirements Not Met, Review Board Denial, No Follow Up, No Longer Interested, Awarded up to Cost of Attendance&quot;)) &amp;&amp;  $RecordType.Name =&apos;Scholarship&apos;</errorConditionFormula>
        <errorDisplayField>FundDenialReason__c</errorDisplayField>
        <errorMessage>Please make sure the application has been properly reviewed by checking to see if they are qualified, have interviewed and that the application has been reviewed.</errorMessage>
    </validationRules>
