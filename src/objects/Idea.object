    <validationRules>
        <fullName>CategoryRequired</fullName>
        <active>true</active>
        <description>Make the category field mandatory</description>
        <errorConditionFormula>ISBLANK(Categories)</errorConditionFormula>
        <errorMessage>Please choose the department needed to help implement your idea.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IdeaClose</fullName>
        <active>true</active>
        <description>Close Idea once its status is changed</description>
        <errorConditionFormula>IF(Closed__c = true, IsMerged = false,  IsMerged = true)</errorConditionFormula>
        <errorMessage>This idea is closed. Please read the comment thread for more information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UniversityGoalRequired</fullName>
        <active>true</active>
        <description>Make university goal field required</description>
        <errorConditionFormula>ISBLANK(UniversityGoal__c)</errorConditionFormula>
        <errorMessage>Please choose at least one university goal that your idea will support.</errorMessage>
    </validationRules>
