    <webLinks>
        <fullName>Mentor_Leave</fullName>
        <availability>online</availability>
        <description>When ever  mentor will be out on extended
leave or separation from WGU or change in role, etc. The students assigned to this mentor will be automatically switched to other mentors.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Remove All Case Loads</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>// What this button do:
//          by click this button ,this mentor will leave for vacation or sick and not available. 
//          all his/her current assignment will be switched to other mentor who is capable for this job unless he/she is the only 
//                      mentor for this course.


{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/27.0/apex.js&quot;)}  
//{!REQUIRESCRIPT(&quot;/resource/1350084909000/extjs_v407/ext-all.js&quot;)}
 
try{ 
	var result = sforce.apex.execute(&quot;CourseMentorStudentAssignmentUtility&quot;, 
              &quot;isCaseLoadManager&quot;, {}); 
	if(result==&apos;true&apos;)
	{
 		if(confirm( &quot;This action will remove all of the Case Loads for {!Mentor__c.Name} and re-distributes the students to other Course Mentors.\n{!Mentor__c.Name}’s availability for Case Loads will also be turned off at this time. \nDo you want to continue?&quot; )) 
 		{ 
 		  result = sforce.apex.execute(&quot;CourseMentorStudentAssignmentUtility&quot;, 
                      &quot;reassignmentsWhenMentorLeave&quot;, {mentor: &quot;{!Mentor__c.PIDM__c}&quot;});
                      //  window.alert(result);
                      if(result==&apos;true&apos;)
                      {
                         window.alert(&quot;SUCCESS! {!Mentor__c.Name} &apos;s all students has been re-assigned to others.&quot;);
                      }
                      else
                      {
                          window.alert(&quot;Failed! Failed to re-assign {!Mentor__c.Name}&apos;s students&quot;);
                      }
		}

	}else
	{
		window.alert(&quot;Failed! This action is limited to Program Managers in Course Mentoring and Senior Managers.&quot;);
	}
 
}catch(e)
{
       window.alert(&quot;Failed! This action is limited to Program Managers in Course Mentoring and Senior Managers.&quot;);
}</url>
