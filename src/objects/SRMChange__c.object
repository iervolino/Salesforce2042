    <validationRules>
        <fullName>AdminApprovalRequired</fullName>
        <active>true</active>
        <description>Do not allow the change to be completed until an admin has approved.</description>
        <errorConditionFormula>AND(
ApprovedByAdmin__c = false,
text(ChangeRequestStatus__c) = &apos;Completed&apos;, 
OR(
INCLUDES(ChangeType__c, &apos;Formula Field&apos;),
INCLUDES(ChangeType__c, &apos;Field on Existing Object&apos;),
INCLUDES(ChangeType__c, &apos;Workflow&apos;),
INCLUDES(ChangeType__c, &apos;Picklist Value Replacement&apos;)
)
)</errorConditionFormula>
        <errorDisplayField>ApprovedByAdmin__c</errorDisplayField>
        <errorMessage>This type of change needs to be approved by an Admin prior to completion.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MakeChangeTimeRequiredWhenComplete</fullName>
        <active>true</active>
        <description>This validation rule makes the change time required whenever a change is marked as complete.</description>
        <errorConditionFormula>AND
(
text(ChangeRequestStatus__c) = &apos;Completed&apos;, 
ISNULL(ChangeTime__c) = TRUE
)</errorConditionFormula>
        <errorDisplayField>ChangeTime__c</errorDisplayField>
        <errorMessage>Please enter the time the change was made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MakeReasonDeniedRequiredWhenDenied</fullName>
        <active>true</active>
        <description>This validation rule makes the reason denied required whenever a change is marked as denied.</description>
        <errorConditionFormula>AND
(
text(ChangeRequestStatus__c) = &apos;Denied&apos;, 
ISNULL( ReasonDenied__c ) = TRUE
)</errorConditionFormula>
        <errorDisplayField>ReasonDenied__c</errorDisplayField>
        <errorMessage>Please enter the reason this request was denied.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MakeTypeRequiredWhenComplete</fullName>
        <active>true</active>
        <description>This validation rule makes the change type required whenever a change is marked as complete.</description>
        <errorConditionFormula>AND
(
text(ChangeRequestStatus__c) = &apos;Completed&apos;, 
ISNULL( ChangeType__c ) = TRUE
)</errorConditionFormula>
        <errorDisplayField>ChangeType__c</errorDisplayField>
        <errorMessage>Please enter the type of change that was made.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MakeWorkByRequiredWhenComplete</fullName>
        <active>true</active>
        <description>This validation rule makes the workcompletedby__c required whenever a change is marked as complete.</description>
        <errorConditionFormula>AND
(
text(ChangeRequestStatus__c) = &apos;Completed&apos;, 
ISBLANK( WorkCompletedBy__c ) = TRUE 
)</errorConditionFormula>
        <errorDisplayField>WorkCompletedBy__c</errorDisplayField>
        <errorMessage>Please enter the user that made the change.</errorMessage>
    </validationRules>
