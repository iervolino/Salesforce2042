    <webLinks>
        <fullName>AppealEvaluation</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Appeal Evaluation</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&apos;/jslibrary/&apos; &amp; LEFT(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(TEXT(NOW()),&apos;:&apos;,&apos;&apos;),&apos;-&apos;,&apos;&apos;),&apos; &apos;,&apos;&apos;),10) &amp; &apos;/sfdc/VFRemote.js&apos;)}{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}{!REQUIRESCRIPT(&apos;/resource/&apos; &amp; LEFT(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(TEXT(NOW()),&apos;:&apos;,&apos;&apos;),&apos;-&apos;,&apos;&apos;),&apos; &apos;,&apos;&apos;),10) &amp; &apos;000/extjs_v407/builds/ext-all-sandbox.js&apos;)}{!REQUIRESCRIPT(&apos;/resource/&apos; &amp; LEFT(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(TEXT(NOW()),&apos;:&apos;,&apos;&apos;),&apos;-&apos;,&apos;&apos;),&apos; &apos;,&apos;&apos;),10) &amp; &apos;000/WGUScriptMgr&apos;)}
(function () {

    Ext4.define(&apos;WGU.view.AppealDlg&apos;, {
        extend: &apos;Ext.window.Window&apos;,

        id: &apos;wguAppealDialog&apos;,
        activeItem: 0,
        height: 125,
        width: 415,
        layout: {
            type: &apos;card&apos;
        },
        title: &apos;Appeal this Transcript Evaluation?&apos;,
        modal: true,

        initComponent: function() {
            var me = this;

            Ext.applyIf(me, {
                items: [
                    {
                        xtype: &apos;container&apos;,
                        id: &quot;appealConfirmation&quot;,
                        layout: {
                            type: &apos;absolute&apos;
                        },
                        items: [
                            {
                                xtype: &apos;label&apos;,
                                x: 10,
                                y: 20,
                                text: &apos;Please confirm that you want to appeal this transcript evaluation.&apos;
                            },
                            {
                                xtype: &apos;button&apos;,
                                x: 200,
                                y: 60,
                                text: &apos;Confirm&apos;,
                                width: 90,
                                listeners: {
                                    click: {
                                        fn: me.onConfirmButtonClick,
                                        scope: me
                                    }
                                }
                            },
                            {
                                xtype: &apos;button&apos;,
                                x: 300,
                                y: 60,
                                text: &apos;Cancel&apos;,
                                width: 90,
                                listeners: {
                                    click: {
                                        fn: me.onCancelButtonClick,
                                        scope: me
                                    }
                                }
                            }
                        ],
                        listeners: {
                            afterlayout: {
                                fn: me.onWindowAfterLayout,
                                scope: me
                            }
                        }
                    },
                    {
                        xtype: &apos;container&apos;,
                        id: &quot;appealWait&quot;
                    }
                ]
            });

            me.callParent(arguments);
        },


        onCancelButtonClick: function(button, e, eOpts) {
            appealDlg.close();
        },

        onConfirmButtonClick: function(button, e, eOpts) {
            appealDlg.getLayout().setActiveItem(1);
            Ext4.getCmp(&quot;appealWait&quot;).setLoading(&quot;Appealing Transcript Eval...&quot;);

            Ext4.Function.defer( function() {

                try {
                    var student = sforce.connection.query(&quot;SELECT Id,FirstName,LastName from Contact WHERE StudentID__c = \&apos;&quot; +wguStudentTranscript.StudentID__c + &quot;\&apos; LIMIT 1&quot;).records;
                    student.Name = student.FirstName + &quot; &quot; + student.LastName;
                    var studentUserId = sforce.connection.query(&quot;SELECT Id from User WHERE ContactId = \&apos;&quot; + student.Id + &quot;\&apos; LIMIT 1&quot;).records.Id;
                    var transcriptRecordTypeId = sforce.connection.query(&quot;SELECT Id FROM RecordType WHERE Name = &apos;Transcripts&apos; AND SobjectType = &apos;Case&apos; LIMIT 1&quot;).records.Id;
                    var transcriptQueueId = sforce.connection.query(&quot;SELECT QueueId FROM QueueSobject WHERE Queue.Name = &apos;Transcripts&apos; LIMIT 1&quot;).records.QueueId;
                    if ( Ext4.isChrome ) {
                        console.log ( &quot;student: &quot; );
                        console.log ( student );
                        console.log ( &quot;studentUserId: &quot; + studentUserId );
                        console.log ( &quot;transcriptRecordTypeId: &quot; + transcriptRecordTypeId );
                        console.log ( &quot;transcriptQueueId: &quot; + transcriptQueueId );
                    }

                    //insert a new Case record
                    var newCase = new sforce.SObject(&quot;Case&quot;);
                    newCase.RecordTypeId = transcriptRecordTypeId;
                    newCase.ContactId = student.Id;
                    newCase.TranscriptTeamURL__c = &quot;https://&quot;+ location.hostname + &quot;/&quot; + wguStudentTranscript.Id;
                    newCase.Reason = &quot;Appeal&quot;;
                    newCase.TransferEvaluation__c = wguStudentTranscript.Id;
                    newCase.Subject = &apos;Transcript Appeal for &apos; + student.Name;
                    //newCase.ServiceDescription__c = &apos;&apos;;
                    //newCase.RequestComments__c = &apos;&apos;;
                    newCase.IsVisibleInSelfService = true;

                    var saveResult = sforce.connection.create([newCase]);
                    if ( saveResult ) {
                        if (saveResult[0].success == &quot;true&quot;) {
                            newCase.Id = saveResult[0].id;
                            if ( Ext4.isChrome ) {
                                console.log ( &quot;Case Create succeeded: &quot;  );
                                console.log ( saveResult[0] );
                            }

                            //insert a new StudentCase__c record
                            var studentCase = new sforce.SObject(&quot;StudentCase__c&quot;);
                            studentCase.Contact__c = newCase.ContactId;
                            studentCase.TransferEvaluation__c = wguStudentTranscript.Id;
                            studentCase.OwnerId = studentUserId;
                            studentCase.Case__c = newCase.Id;
                            studentCase.ParentURL__c = newCase.TranscriptTeamURL__c;
                            studentCase.StudentCaseReason__c  = newCase.Reason;
                            //studentCase.ServiceDescription__c = &apos;&apos;;
                            //studentCase.Comments__c = &apos;&apos;;

                            saveResult = sforce.connection.create([studentCase]);
                            if ( saveResult ) {
                                if (saveResult[0].success == &quot;true&quot;) {
                                    if ( Ext4.isChrome ) {
                                        console.log ( &quot;StudentCase__c Create succeeded: &quot;  );
                                        console.log ( saveResult[0] );
                                    }
                                    studentCase.Id = saveResult[0].id;

                                    //Update Case with Queue Assignment
                                    //Assign case to student as they need to take action first
                                    newCase.OwnerId = studentUserId;
                                    var saveResult = sforce.connection.update([newCase]);
                                    if (saveResult[0].success == &quot;true&quot;) {

                                        if ( Ext4.isChrome ) {
                                            console.log ( &quot;Case Update succeeded: &quot;  );
                                            console.log ( saveResult[0] );
                                        }

                                        //Update the current TransferEvaluation__c record with the Appealed status
                                        var evalUpdate = new sforce.SObject(&quot;TransferEvaluation__c&quot;);
                                        evalUpdate.Id = wguStudentTranscript.Id;
                                        evalUpdate.EvaluationStatus__c = &quot;Appealed&quot;;
                                        saveResult = sforce.connection.update([evalUpdate]);
                                        if ( saveResult ) {
                                            if (saveResult[0].success == &quot;true&quot;) {
                                                Ext4.Msg.show({
                                                    title:&apos;Appeal Creation Successful&apos;,
                                                    msg: &apos;Case records for new Appeal created successfully:&amp;nbsp;&amp;nbsp;&lt;a target=&quot;&apos;+newCase.Id+&apos;&quot; href=&quot;https://&apos; + location.host + &quot;/&quot; + newCase.Id  + &apos;&quot;&gt;New Case Link&lt;/a&gt;&apos;,
                                                    buttons: Ext4.Msg.OK,
                                                    icon: Ext4.MessageBox.INFO,
                                                    fn: function(btn, text) {
                                                        location = &quot;/&quot; + wguStudentTranscript.Id;
                                                    }
                                                });
                                                if ( Ext4.isChrome ) {
                                                    console.log ( &quot;TransferEvaluation__c Update succeeded: &quot;  );
                                                    console.log ( saveResult[0] );
                                                }
                                            } else {
                                                if ( Ext4.isChrome ) {
                                                    console.log ( &quot;TransferEvaluation__c Update failed: &quot;  );
                                                    console.log ( saveResult[0] );
                                                }
                                            }

                                        }
                                    } else {
                                        Ext4.Msg.show({
                                            title:&apos;Appeal Creation Failed [Case UPDATE, Queue Assign]&apos;,
                                            msg: saveResult[0].errors.message,
                                            buttons: Ext4.Msg.OK,
                                            icon: Ext4.MessageBox.ERROR
                                        });
                                        if ( Ext4.isChrome ) {
                                            console.log ( &quot;Case Update failed: &quot;  );
                                            console.log ( saveResult[0] );
                                        }
                                    }
                                } else {
                                    Ext4.Msg.show({
                                        title:&apos;Appeal Creation Failed [StudentCase__c INSERT]&apos;,
                                        msg: saveResult[0].errors.message,
                                        buttons: Ext4.Msg.OK,
                                        icon: Ext4.MessageBox.ERROR
                                    });
                                    if ( Ext4.isChrome ) {
                                        console.log ( &quot;StudentCase__c Create failed: &quot; );
                                        console.log ( saveResult[0] );
                                    }
                                }
                            }
                        } else {
                            Ext4.Msg.show({
                                title:&apos;Appeal Creation Failed [Case INSERT]&apos;,
                                msg: saveResult[0].errors.message,
                                buttons: Ext4.Msg.OK,
                                icon: Ext4.MessageBox.ERROR
                            });
                            if ( Ext4.isChrome ) {
                                console.log ( &quot;Case Create failed: &quot; );
                                console.log ( saveResult[0] );
                            }

                        }

                    }

                } finally {
                    Ext4.getCmp(&quot;appealWait&quot;).setLoading(false);
                    try { appealDlg.close(); } catch(e) {}
                }

            }, 50);

        },

        onWindowAfterLayout: function(container, layout, eOpts) {
        }


    });

    wguStudentTranscript = { Id: &quot;{!TransferEvaluation__c.Id}&quot;, EvaluationStatus__c: &quot;{!TransferEvaluation__c.EvaluationStatus__c}&quot;, StudentID__c:&quot;{!TransferEvaluation__c.StudentID__c}&quot; };

