    <fields>
        <fullName>ExpeditePriority__c</fullName>
        <description>Formula to determine the priority level</description>
        <externalId>false</externalId>
        <formula>IF( OR(TEXT(EvaluationStatus__c) = &quot;Sent&quot;, TEXT(EvaluationStatus__c) = &quot;Not Eligible&quot;), &quot;S&quot;,

IF(Opportunity__r.MilitaryScholarshipPriority__c = true, &quot;EM/S&quot;,

IF(Opportunity__r.LateAdd__c = true, &quot;Late Add&quot;,

IF(Opportunity__r.ExpediteTranscriptEvaluation__c = True, &quot;EC&quot;,

IF (OR(TEXT(ExpediteReason__c)  = &quot;Late Add&quot;, 
            TEXT(ExpediteReason__c)  = &quot;Military&quot;, 
            TEXT(ExpediteReason__c)  = &quot;Scholarship&quot;, 
            TEXT(ExpediteReason__c)  = &quot;Service Failure&quot;, 
            NOW() - Opportunity__r.TranscriptDate__c &gt;= 30), &quot;A&quot;,

IF(OR(TEXT(ExpediteReason__c) = &quot;Degree Plan&quot;,
            TEXT(ExpediteReason__c)  = &quot;Student Request&quot;, 
             NOW() - Opportunity__r.TranscriptDate__c &gt;= 15), &quot;B&quot;,

IF(TEXT(ExpediteReason__c)  = &quot;EWB&quot;, &quot;EWB&quot;,

IF(NOW() - Opportunity__r.TranscriptDate__c &gt;= 7, &quot;C&quot;, 

&quot;N&quot;))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Priority</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
