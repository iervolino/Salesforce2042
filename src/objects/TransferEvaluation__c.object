        var ext4cssUrl = &quot;/resource/{!LEFT(SUBSTITUTE(SUBSTITUTE(SUBSTITUTE(TEXT(NOW()),&apos;:&apos;,&apos;&apos;),&apos;-&apos;,&apos;&apos;),&apos; &apos;,&apos;&apos;),10)}000/extjs_v407/resources/css/ext-sandbox.css&quot;;
        Ext4.WGUScriptMgr.loadCss( {
            url: ext4cssUrl,
