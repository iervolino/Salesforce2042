    function onReady() {
        if ( Ext4.isEmpty(Ext4.util.CSS.getRule(&apos;.x4-shim&apos;, true)) ) {

            Ext4.callback( function(){
                    doDlg();
                },
                window, null, 2000 );

        } else {
            doDlg();
        }

    }

    function doDlg() {
        if ( wguStudentTranscript.EvaluationStatus__c != &apos;Appealed&apos; ) {
            appealDlg = Ext4.create(&quot;WGU.view.AppealDlg&quot;,{});
            appealDlg.show();
