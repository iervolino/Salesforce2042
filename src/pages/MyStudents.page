                    ||(me.AtRiskScore==3||me.AtRiskScore==0||me.AtRiskScore==null)) || !me.isStudentMentor;
        
        if ( !me.saveNoteButton ) {
            me.saveNoteButton = Ext.getCmp('SaveNoteButtonDlg');
        }

