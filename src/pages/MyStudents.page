        if(courseList==null|| courseList.CourseLong__c==null || courseList.CourseLong__c=='' )
        {
            return;
        }
         
        var courses=courseList.CourseLong__c;
        
        if(courses==null||courses=='')
        {
            return;
        }else if(courses == 'all')
