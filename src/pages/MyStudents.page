                                                title:'Save Student Performance Program Exception',
                                                msg: (e.message? e.message: e) + '<br/>Reference: saveNote() -> Save Survey',
                                                buttons: Ext.Msg.OK,
                                                icon: Ext.Msg.ERROR
                                            });
                                        }
                                    }
                                } else {
                                    Ext.getCmp('NoteHTMLTextEditorDlg').setLoading('Save success');
