                    icon: Ext.Msg.INFO
                });
                Ext.select('iframe').elements[0].focus();
                me.NoteHTMLTextEditorDlg.focus();
            } else if ( !performance ) {
                Ext.MessageBox.show({
                    title:'Performance Survey Responses Required',
                    msg: 'As this student\'s mentor, please answer all performance survey questions.',
                    buttons: Ext.Msg.OK,
                    icon: Ext.Msg.INFO
                });
