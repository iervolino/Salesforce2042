                                                dialog.close();
                                            }, 1500);
                                        } else {
                                            if ( Ext.isChrome ) {
                                                console.log( 'Survey Save/Creation Unsuccessfull: ' );
                                                console.log( surveyResult );
                                            }
                                        }
                                    } catch (e) {
